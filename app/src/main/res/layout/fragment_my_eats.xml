<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".myeats.MyEatsFragment">

    <TextView
        android:id="@+id/tv_my_name"
        android:layout_width="wrap_content"
        android:layout_height="33dp"
        android:layout_marginStart="141dp"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/pretendard_variable"
        android:text="@string/my_name"
        android:textFontWeight="400"
        android:textSize="28sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_phone_number"
        android:layout_width="wrap_content"
        android:layout_height="16dp"
        android:layout_marginStart="135dp"
        android:layout_marginTop="12dp"
        android:text="@string/phone_number"
        android:textColor="@color/phone_number"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_my_name" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_my_eats_my_information"
        android:layout_width="287dp"
        android:layout_height="63dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_phone_number">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/con_mypage1"
            android:layout_width="wrap_content"
            android:layout_height="63dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="33dp"
                android:layout_marginStart="34dp"
                android:fontFamily="@font/pretendard_variable"
                android:text="0"
                android:textFontWeight="400"
                android:textSize="28sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/pretendard_variable"
                android:text="내가 남긴 리뷰"
                android:textSize="13sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />


        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/con_mypage2"
            android:layout_width="wrap_content"
            android:layout_height="63dp"
            android:layout_marginStart="36dp"
            app:layout_constraintStart_toEndOf="@id/con_mypage1"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:layout_width="17dp"
                android:layout_height="33dp"
                android:layout_marginStart="30dp"
                android:fontFamily="@font/pretendard_variable"
                android:text="0"
                android:textFontWeight="400"
                android:textSize="28sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/pretendard_variable"
                android:text="도움이 됐어요"
                android:textSize="13sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/con_mypage3"
            android:layout_width="wrap_content"
            android:layout_height="63dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:layout_width="17dp"
                android:layout_height="33dp"
                android:layout_marginEnd="14dp"
                android:fontFamily="@font/pretendard_variable"
                android:text="0"
                android:textFontWeight="400"
                android:textSize="28sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/pretendard_variable"
                android:text="즐겨찾기"
                android:textSize="13sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/iv_my_eats_move_detail"
        android:layout_width="306dp"
        android:layout_height="42dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="18dp"
        android:background="@drawable/my_eats_button"
        android:backgroundTint="@null"
        android:fontFamily="@font/pretendard_variable"
        android:text="자세히 보기"
        android:textColor="#02AFFE"
        android:textSize="16sp"
        android:theme="@style/Widget.AppCompat.Button.Borderless"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cl_my_eats_my_information" />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_my_eats_move_detail"
        android:layout_width="122dp"
        android:layout_height="30dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="28dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_my_eats_move_detail">

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/img_heart"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_favorites"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/pretendard_variable"
            android:text="즐겨찾기"
            android:textFontWeight="400"
            android:textSize="21sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>